{
    "name": "hyperf/grpc-server",
    "description": "A GRPC Server for Hyperf.",
    "license": "Apache-2.0",
    "keywords": [
        "php",
        "swoole",
        "hyperf",
        "grpc",
        "grpc-server"
    ],
    "support": {
    },
    "require": {
        "php": ">=7.2",
        "nikic/fast-route": "^1.3",
        "hyperf/framework": "dev-master",
        "hyperf/utils": "dev-master",
        "hyperf/http-server": "dev-master",
        "google/protobuf": "^3.6.1",
        "grpc/grpc": "^1.15"
    },
    "require-dev": {
        "malukenho/docheader": "^0.1.6",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0.0",
        "friendsofphp/php-cs-fixer": "^2.9"
    },
    "suggest": {
        "ext-grpc": "grpc/grpc require grpc extension.",
        "ext-protobuf": "For better performance, install the protobuf C extension."
    },
    "autoload": {
        "files": [
        ],
        "psr-4": {
            "Hyperf\\GrpcServer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
        },
        "hyperf": {
            "config": "Hyperf\\GrpcServer\\ConfigProvider"
        }
    },
    "bin": [
    ],
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1",
        "test": "phpunit --colors=always"
    }
}
